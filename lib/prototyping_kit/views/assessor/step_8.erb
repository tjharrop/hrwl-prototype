<article>
  <h1>Assessment results</h1>

  <form action="/assessor/step-9" method="get">
<h2>Calculations results</h2>
    <fieldset>
      <div class="form-group">
        <label for="calculations-start">Start time</label>
        <input type="text"
                name="calculations_start"
                id="calculations-start"
                class="form-control"
                style="width: 20em;"
                value="<%= data[:calculations_start] %>">
      </div>

      <div class="form-group">
        <label for="calculations-end">End time</label>
        <input type="text"
                name="calculations_end"
                id="calculations-end"
                class="form-control"
                style="width: 20em;"
                value="<%= data[:calculations_end] %>">
      </div>

    <div class="form-group">
      <label for="calculations-extra">Notes or previous AS</label>
      <input type="text"
              name="calculations_extra"
              id="calculations-extra"
              class="form-control"
              value="<%= data[:calculations_extra] %>">
    </div>

</fieldset>



    <fieldset class="actions">
      <button type="submit">Next step</button>
    </fieldset>
  </form>
</article>
